{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "definitions": {
        "BindingTarget": {
            "enum": [
                "attribute",
                "class",
                "content",
                "css",
                "event",
                "property"
            ],
            "type": "string"
        },
        "IControl": {
            "properties": {
                "html": {
                    "type": "string"
                },
                "properties": {
                    "$ref": "#/definitions/Record<string,{type:string;values?:string[]|undefined;default?:any;}>"
                },
                "script": {
                    "type": "string"
                },
                "settings": {
                    "$ref": "#/definitions/IControlSettings"
                },
                "style": {
                    "type": "string"
                },
                "typeScript": {
                    "type": "string"
                }
            },
            "type": "object"
        },
        "IControlSettings": {
            "properties": {
                "height": {
                    "type": "string"
                },
                "useGlobalStyle": {
                    "description": "CustomControl does include Global Style in it's shadowroot",
                    "type": "boolean"
                },
                "width": {
                    "type": "string"
                }
            },
            "type": "object"
        },
        "ICustomControlScript": {
            "type": "object"
        },
        "IGlobalConfig": {
            "type": "object"
        },
        "IIobrokerWebuiBinding": {
            "properties": {
                "converter": {
                    "$ref": "#/definitions/Record<string,any>"
                },
                "events": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                },
                "expression": {
                    "type": "string"
                },
                "inverted": {
                    "type": "boolean"
                },
                "signal": {
                    "type": "string"
                },
                "target": {
                    "$ref": "#/definitions/BindingTarget"
                },
                "twoWay": {
                    "type": "boolean"
                },
                "type": {
                    "type": "string"
                }
            },
            "type": "object"
        },
        "IScreen": {
            "properties": {
                "html": {
                    "type": "string"
                },
                "script": {
                    "type": "string"
                },
                "settings": {
                    "$ref": "#/definitions/IScreenSettings"
                },
                "style": {
                    "type": "string"
                },
                "typeScript": {
                    "type": "string"
                }
            },
            "type": "object"
        },
        "IScreenSettings": {
            "properties": {
                "height": {
                    "type": "string"
                },
                "scale": {
                    "anyOf": [
                        {
                            "enum": [
                                "fit",
                                "noneNoScroll"
                            ],
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ]
                },
                "width": {
                    "type": "string"
                }
            },
            "type": "object"
        },
        "IScriptMultiplexValue": {
            "properties": {
                "name": {
                    "description": "Name of the ioBroker object or the property of the component",
                    "format": "signal",
                    "type": "string"
                },
                "source": {
                    "enum": [
                        "property",
                        "signal"
                    ],
                    "type": "string"
                }
            },
            "type": "object"
        },
        "IWebUiConfig": {
            "properties": {
                "globalConfig": {
                    "$ref": "#/definitions/IGlobalConfig"
                },
                "globalScript": {
                    "type": "string"
                },
                "globalStyle": {
                    "type": "string"
                },
                "globalTypeScript": {
                    "type": "string"
                }
            },
            "type": "object"
        },
        "Record<string,any>": {
            "type": "object"
        },
        "Record<string,{type:string;values?:string[]|undefined;default?:any;}>": {
            "type": "object"
        }
    }
}

