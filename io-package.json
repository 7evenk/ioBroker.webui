    "icon": "logo.png",
    "extIcon": "https://raw.githubusercontent.com/iobroker-community-adapters/ioBroker.webui/master/admin/logo.png",
    "connectionType": "local",
    "dataSource": "push",
    "platform": "Javascript/Node.js",
    "loglevel": "info",
    "enabled": true,
    "compact": false,
    "keywords": [
      "GUI",
      "graphical",
      "scada",
      "webcompoents",
      "HTML"
    ],
