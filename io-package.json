{
    "common": {
        "name": "webui",
        "version": "0.0.1",
        "title": "webui",
        "desc": {
            "en": "Graphical user interface for iobroker using Webcomponents"
        },
        "platform": "Javascript/Node.js",
        "loglevel": "info",
        "enabled": true,
        "keywords": [
            "GUI",
            "graphical",
            "scada",
            "webcompoents",
            "HTML"
        ],
        "readme": "",
        "authors": [
            "jogibear9988 <jochen.kuehner@gmx.de>"
        ],
        "localLink": "%web_protocol%://%ip%:%web_port%/webui/runtime.html",
        "license": "MIT",
        "dependencies": [
            {
                "web": ">=3.4.16"
            }
        ],
        "restartAdapters": [
            "webui"
        ],
        "serviceStates": false,
        "singleton": true,
        "type": "visualization",
        "highlight": true,
        "noConfig": true,
        "materialize": true,
        "onlyWWW": true,
        "nogit": true,
        "mode": "extension",
        "native": {
            "webInstance": "*"
        },
        "welcomeScreen": [
            {
                "link": "webui/runtime.html",
                "name": "webui runtime",
                "img": "vis/img/favicon.png",
                "color": "#ffe9c8",
                "order": 0
            },
            {
                "link": "webui/index.html",
                "name": "webui editor",
                "img": "vis/img/faviconEdit.png",
                "color": "#c8ffe1",
                "order": 1
            }
        ]
    }
}