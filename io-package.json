    "connectionType": "local",
    "dataSource": "push",
    "platform": "Javascript/Node.js",
    "loglevel": "info",
    "enabled": true,
    "compact": false,
    "keywords": [
      "GUI",
      "graphical",
      "scada",
      "webcompoents",
      "HTML"
